public class IfElse{
	public static void main(String[] args){
		
		for(int i = 1;i<= 5;i++){ //row
			for(int j =1;j<=i;j++){ // coloumn
				System.out.print("*  ");
			}
			
			System.out.println();
		}
	}
}                            // *
